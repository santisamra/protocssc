<?xml version="1.0" encoding="UTF-8"?>

<config>
	<filters>
		<filter>
			<conditions>
				<browser>Firefox/3.6</browser>
			</conditions>
			<actions>
				<blocked-IPs>
					<IP>192.168.1.*</IP>
				</blocked-IPs>
				<blocked-URIs>
					<URI>http://www.2leep.com/*</URI>
				</blocked-URIs>
				<block-all-accesses>FALSE</block-all-accesses>
				<transform>
					<l33t>FALSE</l33t>
					<images180>FALSE</images180>
				</transform>
			</actions>
		</filter>
	</filters>
</config>