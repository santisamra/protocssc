<?xml version="1.0" encoding="UTF-8"?>

<config>
	<filters>
		<filter>
			<conditions>
				<browser>Firefox/3.6</browser>
			</conditions>
			<actions>
				<blocked-IPs>
					<IP>192.168.1.1</IP>
					<IP>85.62.96.168</IP>
				</blocked-IPs>
				<blocked-URIs>
					<URI>taringa.net</URI>
				</blocked-URIs>
				<block-all-accesses>FALSE</block-all-accesses>
				<transform>
					<l33t>FALSE</l33t>
					<images180>FALSE</images180>
				</transform>
				<max-content-length>102400</max-content-length>
				<blocked-MediaTypes>
					<MediaType>image/png</MediaType>
				</blocked-MediaTypes>
			</actions>
		</filter>
	</filters>
</config>